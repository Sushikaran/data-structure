#include <stdio.h>

int findElement(int arr[], int n, int element)
 {
 	printf("sushikaran\n192324018\n");
    for (int i = 0; i < n; i++) {
        if (arr[i] == element) {
            return i;
        }
    }
    return -1;
}

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int element = 3;
    
    int location = findElement(arr, n, element);
    
    if (location != -1) {
        printf("The element %d is located at index %d\n", element, location);
    } else {
        printf("The element %d is not found in the array\n", element);
    }
    
    return 0;
}
